algoritmo "Ex_6"
// Exercicio 6 - Crie um Algoritmo que calcule e
// exiba a soma dos primeiros 3 n�meros.
var
    num1: real
    num2: real
    num3: real
    result: real
inicio
    escreva("Digite o primeiro numero: ")
    leia(num1)
    escreva("Digite o segundo numero: ")
    leia(num2)
    escreva("Digite o terceiro numero: ")
    leia(num3)
    result <- (num1 + num2 + num3)
    escreva("O resultado é: ", result)

fimalgoritmo